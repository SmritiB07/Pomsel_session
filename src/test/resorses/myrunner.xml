<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="My Runner Suite" thread-count="3" parallel="tests" verbose="4">
	
	<test name="Google Test">
		<parameter name="url" value="https://www.google.com"/>
		<parameter name="browser" value="chrome"/>
		<classes>
			<class name="mytests.GoogleTest"/>
			</classes>
	</test>
	
	<test name="Orange HRM Test">
		<parameter name="url" value="https://www.orangehrm.com"/>
		<parameter name="browser" value="EDGE"/>
		<classes>
			<class name="mytests.OrangeHRMTest"/>
			</classes>
	</test>
	
	<test name="Open Cart Test">
		<parameter name="url" value="https://www.amazon.in/gp/cart/view.html?ref_=nav_cart"/>
		<parameter name="browser" value="EDGE"/>
		<classes>
			<class name="mytests.openCartTest"/>
			</classes>
	</test>
	
	
</suite>
	